server.port=8081

#Version number from pom.xml
app.version=@project.version@

#telegram properties
telegram.bot.name=
telegram.bot.token=

#DB properties
######################################################
spring.datasource.url=
spring.datasource.username=
spring.datasource.password=
spring.datasource.driverClassName=org.h2.Driver
spring.jpa.database-platform=org.hibernate.dialect.H2Dialect

#Populates the data after the schema is generated by Hibernate
spring.jpa.defer-datasource-initialization=true

#Accessing the H2 Console
spring.h2.console.enabled=true

#http://localhost:{server.port}/db-console
spring.h2.console.path=/db-console

#Prevent trace output
spring.h2.console.settings.trace=false

#Disable remote access
spring.h2.console.settings.web-allow-others=false
##################################################################

# logback properties
##################################################################
logging.file.name=logs/spring-boot-logging.log

#The maximum number of archive log files to keep
logging.logback.rollingpolicy.max-history=7

#The maximum size of log file before it is archived.
logging.logback.rollingpolicy.max-file-size=10MB

#The maximum size of log file before it is archived.
logging.logback.rollingpolicy.total-size-cap=10MB
##################################################################

